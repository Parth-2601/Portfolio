{
    "version": 2,
    "builds": [
      {
        "src": "email-server/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "dist/", 
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "email-server/server.js"
      }
    ]
  }