{
    "version": 2,
    "builds": [
      {
        "src": "email-server/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "src/App.jsx",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "email-server/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/src/App.jsx"
      }
    ]
  }
  